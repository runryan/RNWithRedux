{
  "parser": "babel-eslint",
  "globals": {
    "__DEV__": true,
    "fetch": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018
  },
  "extends": "airbnb",
  "plugins": ["react", "react-native"],
  "rules": {},
  "react/jsx-filename-extension": [
    1,
    {
      "extensions": [".js", ".jsx"]
    }
  ]
}
